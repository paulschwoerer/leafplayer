{
  "name": "leafplayer",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "author": "Paul Schw√∂rer <hello@paulschwoerer.de>",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc --project tsconfig.prod.json",
    "lint": "eslint --ext .ts lib/ test/",
    "lint:fix": "npm run lint -- --fix",
    "test": "ava",
    "test:watch": "ava --watch",
    "make-migration": "knex migrate:make -x ts --migrations-directory=lib/database/migrations"
  },
  "_moduleAliases": {
    "@common": "./common",
    "@mappers": "./lib/mappers",
    "@middlewares": "./lib/middlewares",
    "@query": "./lib/query",
    "@services": "./lib/services",
    "@typings": "./lib/typings"
  },
  "ava": {
    "files": [
      "lib/**/*.test.ts",
      "test/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "./test/prerequisites.ts"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "fastify": "^3.10.1",
    "fastify-static": "^4.2.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.15",
    "md5-file": "^5.0.0",
    "module-alias": "^2.2.2",
    "music-metadata": "^7.6.3",
    "sharp": "^0.27.1",
    "sqlite3": "5.0.0",
    "ua-parser-js": "^0.7.23",
    "uid-safe": "^2.1.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/gulp": "^4.0.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.20",
    "@types/sharp": "^0.27.1",
    "@types/ua-parser-js": "^0.7.35",
    "@types/uid-safe": "^2.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ava": "^3.15.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "execa": "^5.0.0",
    "gulp": "^4.0.2",
    "node-gyp": "^7.1.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
